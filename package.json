{
  "name": "ai-assistant",
  "private": true,
  "version": "1.0.0",
  "description": "ExamineticsSl AI Assistant Dashboard - Human-in-the-Loop Agent Orchestration Interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview",
    "serve": "npm run build && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:debug": "playwright test --debug",
    "test:visual:update": "playwright test --update-snapshots"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@progress/kendo-react-buttons": "^12.0.0",
    "@progress/kendo-react-dialogs": "^12.0.0",
    "@progress/kendo-react-dropdowns": "^12.0.0",
    "@progress/kendo-react-indicators": "^12.0.0",
    "@progress/kendo-react-inputs": "^12.0.0",
    "@progress/kendo-react-layout": "^12.0.0",
    "@progress/kendo-react-notification": "^12.0.0",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@storybook/addon-storyshots-puppeteer": "^7.6.20",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.55.0",
    "postcss": "^8.4.32",
    "puppeteer": "^24.17.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^3.2.4"
  }
}
